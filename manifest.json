{
    "name": "Born",
    "description": "A new tab homepage.",
    "version": "0.1",
    "incognito": "split",
    "permissions": ["storage", "topSites", "unlimitedStorage"],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "chrome_url_overrides": {
        "newtab": "born.html"
    },
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    }
}
